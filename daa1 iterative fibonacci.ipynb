    "num1 = 0\n",
    "num2 = 1\n",
    "nextTerm = 0\n",
    "arr = []\n",
    "\n",
    "print(\"enter number of terms: \")\n",
    "n = int(input())\n",
    "\n",
    "arr.append(num1)\n",
    "arr.append(num2)\n",
    "\n",
    "\n",
